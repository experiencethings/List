                     if d in AddFruits:
                         if(d=='Apple' or d=='Mango' or d=='Grape' or d=='apple' or d=='mango' or d=='grape'):
                             print("This item is already in the list")
                         else:
                             (fruits.append(d))
                             (fruits.sort())
                             print(fruits)
                     else:
                         print("This item cannot be added in this list")
                         
                 elif(c==2):
                     e=str(input("Enter the item:"))
                     if e in fruits:
                         (fruits.remove(e))
                         (fruits.sort())
                         print(fruits)
                     else:
                         print("wrong item selected")
                     
                     
                 
                 break

             elif (x!="Anubhav Jain" and y=="anubhav123"):
                 print("your name might not be correct, please try again")
                 continue

             elif (x=="Anubhav Jain" and y!="anubhav123"):
                 print("your password is incorrect, please try again")
                 continue

             else:
