    b=int(input("Enter a no.:" ))
    if (b==1):
        print("TO EDIT LIST YOU HAVE TO FIRST CONFIRM YOUR IDENTITY")
       
        while True:
             x=str(input("Enter your name:"))
             y=str(input("Enter your password:"))

             if (x=="Anubhav Jain" and y=="anubhav123"):
                 print("Welcome Anubhav, excess granted")
                 print("Select an option:")
                 print("1. Add an item\n2. Remove an item ")
                 c=int(input("Enter a no.:"))
                 if (c==1):
                     AddFruits=['apple', 'apricot avocado','banana','blackberry','blackcurrant','blueberry','boysenberry','cherry','coconut','fig','grape','grapefruit','kiwifruit','lemon','lime','lychee','mandarin','mango','melon','nectarine','orange','papaya','passion fruit','peach','pear','pineapple','plum','pomegranate','quince','raspberry','strawberry','watermelon','apple', 'Apricot Avocado','Banana','Blackberry','Blackcurrant','Blueberry','Boysenberry','Cherry','Coconut','Fig','Grape','Grapefruit','Kiwifruit','Lemon','Lime','Lychee','Mandarin','Mango','Melon','Nectarine','Orange','Papaya','Passion Fruit','Peach','Pear','Pineapple','Plum','Pomegranate','Quince','Raspberry','Strawberry','Watermelon']
